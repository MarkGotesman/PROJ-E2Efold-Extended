	"version": "0.2.0",
	"configurations": [
		{
			"name": "RNAStralign: Short",
			"type": "debugpy",
			"request": "launch",
			"program": "'${workspaceFolder}'/experiment_rnastralign/e2e_learning_stage3.py",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}/experiment_rnastralign",
			"args": "-c config.json --test True"
		},
		{
			"name": "RNAStralign: Long",
			"type": "debugpy",
			"request": "launch",
			"program": "'${workspaceFolder}'/experiment_rnastralign/e2e_learning_stage3_rnastralign_all_long.py",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}/experiment_rnastralign",
			"args": "-c config_long.json --test True"
		},
		{
			"name": "ArchiveII: Short",
			"type": "debugpy",
			"request": "launch",
			"program": "'${workspaceFolder}'/experiment_archiveii/e2e_learning_stage3.py",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}/experiment_archiveii",
			"args": "-c config.json"
		},
		{
			"name": "ArchiveII: Long",
			"type": "debugpy",
			"request": "launch",
			"program": "'${workspaceFolder}'/experiment_archiveii/e2e_learning_stage3_rnastralign_all_long.py",
			"console": "integratedTerminal",
			"cwd": "${workspaceFolder}/experiment_archiveii",
			"args": "-c config_long.json"
		},
		{
			"name": "Current File with CWD",
			"type": "debugpy",
			"request": "launch",
			"cwd": "${fileDirname}",
			"program": "${file}"
		}
	]
}